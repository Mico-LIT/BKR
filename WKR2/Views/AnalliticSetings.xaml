<Window x:Class="WKR2.Views.AnalliticSetings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WKR2.Views"
        mc:Ignorable="d"
        Title="Настройка Аналитики" Height="160" Width="310">
    <StackPanel Name="stackPanel">

        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">Индидуальные номер</TextBlock>
        <ComboBox Name="comboBoxId" SelectedIndex="{Binding Path=Params_1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">ФИО</TextBlock>
        <ComboBox Name="comboBoxFIO" SelectedIndex="{Binding Path=Params_2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="OK" Click="Button_Click"></Button>
    </StackPanel>
</Window>
